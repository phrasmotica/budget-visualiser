TransactionListerEvents="*res://scenes/autoloads/transaction_lister_events.gd"
Log="*res://addons/GoLogger/Log.tscn"
Logger="*res://scenes/autoloads/logger.gd"

[display]

window/size/viewport_width=1440
window/size/viewport_height=900
